<root main_tree_to_execute="LaneDetectBT">
    <BehaviorTree ID="LaneDetectBT">
        <Sequence name="BehaviorTree">
            <Sequence name="Mission 0: Traffic Light">
                <TrafficLight timeout="60.0"/>
            </Sequence>
            <Sequence name="Mission 1: Intersection">
                <LaneDetect mode="center" sign="" duration="5.0" speed="0.12"/> 
                <LaneDetect mode="intersection" duration="300.0" sign="" speed="0.09"/>   
            </Sequence>

            <Sequence name="Mission 2: Construction">
                <LaneDetect mode="center" sign="" duration="5.0" speed="0.12"/> 
                <LaneDetect mode="just-turn-left" duration="60.0" sign="right" speed="0.11"/>  
                <Construction mode="ramp_2"/> 
            </Sequence>

            <Sequence name="Mission 3: Parking">
                <Move mode="forward" until="timer" distance="2.5"/>
                <LaneDetect mode="center" duration="1" sign="" speed="0.1"/>
                <LaneDetect mode="just-turn-left" duration="60.0" sign="left" speed="0.12"/>    
                <Parking mode="dynamic"/>
                <LaneDetect mode="just-turn-left" duration="10.0" sign="left"/>
                <LaneDetect mode="center" duration="2" sign="" speed="0.1"/>
            </Sequence>

            <Sequence name="Mission 4: Level Crossing">
                <LaneDetect mode="cross_level" duration="30.0" sign="" speed="0.08"/>
                <LaneDetect mode="cross_level" duration="300.0" sign="" speed="0.1"/>
                <LaneDetect mode="center" duration="3.0" sign=""/>
                <LaneDetect mode="just-turn-right" duration="60.0" sign="left" speed="0.10"/>  
                <LaneDetect mode="just-turn-left" duration="60.0" sign="right" speed="0.1"/>  
            </Sequence>

            <Sequence name="Mission 5: Tunnel">
                <LaneDetect mode="center" duration="2.5" sign="" speed="0.12"/>
                <Move mode="forward" until="right" distance="0.3"/>
                <Move mode="forward" until="timer" distance="2"/>
                <TunnelNav mode="offline"/>
                <Align by="right"/>
                <Move mode="forward" until="left" distance="0.25"/>
                <Move mode="forward" until="timer" distance="1.0"/>
            </Sequence>

            <Sequence name="Mission 6: Finish">
                <LaneDetect mode="center" duration="5.0" sign="" speed="0.1"/>
            </Sequence>
        </Sequence>
    </BehaviorTree>
</root>